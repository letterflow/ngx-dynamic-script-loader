{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "pnpm"
  },
  "newProjectRoot": "projects",
  "projects": {
    "ngx-dynamic-script-loader": {
      "projectType": "library",
      "root": "projects/ngx-dynamic-script-loader",
      "sourceRoot": "projects/ngx-dynamic-script-loader/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-dynamic-script-loader/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-dynamic-script-loader/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-dynamic-script-loader/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ngx-dynamic-script-loader/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "examples": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "inlineTemplate": true,
          "inlineStyle": true,
          "style": "scss",
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:resolver": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/examples",
      "sourceRoot": "projects/examples/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/examples",
            "index": "projects/examples/src/index.html",
            "browser": "projects/examples/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/examples/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/examples/src/favicon.ico",
              "projects/examples/src/assets"
            ],
            "styles": [
              "projects/examples/src/styles.scss"
            ],
            "scripts": [],
            "server": "projects/examples/src/main.server.ts",
            "prerender": true,
            "ssr": {
              "entry": "projects/examples/server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "examples:build:production"
            },
            "development": {
              "buildTarget": "examples:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "examples:build"
          }
        }
      }
    }
  }
}
